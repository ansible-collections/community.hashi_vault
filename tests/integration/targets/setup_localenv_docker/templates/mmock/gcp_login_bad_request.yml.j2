---
request:
  method: POST|PUT
  path: "/v1/auth/gcp*/login"
  body: '*fail-me-role*'
control:
  priority: 11
response:
  statusCode: 400
  headers:
    Content-Type:
      - application/json
  body: >-
    {
        "errors": [
            "unable to log in with GCP auth: /v1/auth/gcp"
        ]
    }