#jinja2:variable_start_string:'[%', variable_end_string:'%]'
---
request:
  method: POST|PUT
  path: "/v1/auth/azure*/login"
  body: '*fail-me-role*'
control:
  priority: 11
response:
  statusCode: 400
  headers:
    Content-Type:
      - application/json
  body: >-
    {
        "errors": [
            "oidc: expected audience \"https://management.azure.com/\" got [\"https://management.azure.com\"]"
        ]
    }
