# {{ ansible_managed }}
pid_file = "{{ local_temp_dir }}/vault.pid"
{% if true or cryptography_version.stdout is version('1.6', '>=') %}
listener "tcp" {
  tls_key_file = "{{ vault_pki_path }}/privatekey.pem"
  tls_cert_file = "{{ vault_pki_path }}/cert.pem"
  tls_disable  = false
  address = "localhost:8201"
}
{% endif %}
